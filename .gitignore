.vscode/
.idea/
*.swp
*.swo

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore pytest cache
.pytest_cache/

# Ignore coverage reports
htmlcov/
.coverage

# Ignore temporary files
/tmp/
*.tmp