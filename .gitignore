# Ignore Python cache files
__pycache__/
*.py[cod]
*$py.class

# Ignore virtual environment
venv/
env/
.env

# Ignore large/binary model artifacts (train locally); allow code & metadata
models/*
!models/**/model_metadata.json
!models/**/metrics.json
!models/**/holdout_metrics.json
!models/**/classifier_config.json
*.pkl
*.joblib

# Ignore raw/enriched data CSVs except explicit small examples
data/*.csv
!data/sample_tickets.csv
!data/generate_dataset.py

# Ignore logs
*.log

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore pytest cache
.pytest_cache/

# Ignore coverage reports
htmlcov/
.coverage

# Ignore temporary files
/tmp/
*.tmp